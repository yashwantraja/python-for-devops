import sys
def addi(num1,num2):
    addy = num1 + num2
    return addy

def sub(num1,num2):
    sub = num1 - num2
    return sub

def mul(num1,num2):
    mul = num1 * num2
    return mul

num1 = float(sys.argv[1])
operation = sys.argv[2]
num2 = float(sys.argv[3])

if operation == "add":
    output = addi(num1,num2)
    print(output)
